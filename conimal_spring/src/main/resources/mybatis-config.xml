<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.conimal.spring.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.conimal.spring.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.conimal.spring.report.model.vo.Report" alias="Report"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="/mappers/member-mapper.xml"/>
		<mapper resource="/mappers/board-mapper.xml"/>
		<mapper resource="/mappers/report-mapper.xml"/>
	</mappers>

</configuration>
